@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <div class="row">
        <div class="col s7 center-align">
            <h3>Welcome to Lost in the Woods</h3>
            <p>A community that allows people to discover new outdoor places to explore</p>
        </div>
        <div class="col s5"  id="icon-wrapper">
            <img src="data:image/svg+xml;utf8;base64," class="trails-icon" />
        </div>
    </div>
</div>
<div class="row center-align" id="index-links">
    <div class="section col s6">
        <h4>All Trails</h4>
        <p>View all trails that have been added.</p>
        <a href="/Trails" class="waves-effect waves-light btn btn-color">Trails</a>
    </div>
    <div class="section col s6">
        <h4>Add a Trail</h4>
        <p>Add a new trail for others to see.</p>
        <a href="/NewTrail" class="waves-effect waves-light btn btn-color">Add Trail</a>
    </div>
</div>
